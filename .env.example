# =============================================================================
# SecureHST Insights — Environment Configuration
# =============================================================================
# Copy this file to .env and edit the values you need.
# Lines beginning with # are comments. Defaults shown in parentheses.
#
# Boolean values accept: true/1/yes or false/0/no (case-insensitive)
# =============================================================================

# ---------------------------------------------------------------------------
# Required
# ---------------------------------------------------------------------------

# IMPORTANT: Generate a unique secret key for production!
#   python3 -c "import secrets; print(secrets.token_urlsafe(42))"
SUPERSET_SECRET_KEY=CHANGE_ME_TO_A_RANDOM_SECRET

# Admin user password (set on first init)
ADMIN_PASSWORD=admin

# Database password — used for both Superset and the PostgreSQL container
DATABASE_PASSWORD=superset

# ---------------------------------------------------------------------------
# Optional
# ---------------------------------------------------------------------------

# Application port (default: 8088)
#SUPERSET_PORT=8088

# ---------------------------------------------------------------------------
# 1. Branding
# ---------------------------------------------------------------------------
# Customize the application name and logo shown in the navigation bar.
# Logo & favicon are auto-detected from assets/logo.png and assets/favicon.png.
# Override the URL paths below only if you need a different location.

#SUPERSET_APP_NAME=Insights                                         # (Insights)
#SUPERSET_APP_ICON=/static/assets/images/logo.png                   # URL path to logo
#SUPERSET_LOGO_TARGET_PATH=/                                        # (/)
#SUPERSET_LOGO_TOOLTIP=                                             # Hover text
#SUPERSET_LOGO_RIGHT_TEXT=                                          # Text beside logo
#SUPERSET_FAVICON=/static/assets/images/favicon.png                 # URL path to favicon

# ---------------------------------------------------------------------------
# 2. Theme (Ant Design token overrides)
# ---------------------------------------------------------------------------
# Customize the color palette, typography, and sizing.

# Brand logo sizing
#SUPERSET_BRAND_LOGO_HEIGHT=                                        # e.g. 28px
#SUPERSET_BRAND_LOGO_MARGIN=                                        # e.g. 8px 0

# Colors (hex values, e.g. #1677ff)
#SUPERSET_THEME_COLOR_PRIMARY=                                      # Primary brand color
#SUPERSET_THEME_COLOR_ERROR=                                        # Error / danger
#SUPERSET_THEME_COLOR_WARNING=                                      # Warning
#SUPERSET_THEME_COLOR_SUCCESS=                                      # Success
#SUPERSET_THEME_COLOR_INFO=                                         # Informational
#SUPERSET_THEME_COLOR_BG_BASE=                                      # Base background
#SUPERSET_THEME_COLOR_BG_LAYOUT=                                    # Layout background
#SUPERSET_THEME_COLOR_TEXT_BASE=                                    # Base text color
#SUPERSET_THEME_COLOR_LINK=                                         # Link color

# Sizing
#SUPERSET_THEME_BORDER_RADIUS=6                                     # (6)
#SUPERSET_THEME_FONT_SIZE=14                                        # (14)
#SUPERSET_THEME_CONTROL_HEIGHT=32                                   # (32)

# Typography
#SUPERSET_THEME_FONT_FAMILY=                                        # e.g. Inter, sans-serif
#SUPERSET_CUSTOM_FONT_URLS=                                         # Comma-separated URLs

# ---------------------------------------------------------------------------
# 3. UI Navigation Links
# ---------------------------------------------------------------------------
# Customize help and support links shown in the navigation bar.

#SUPERSET_BUG_REPORT_URL=                                           # Bug report form URL
#SUPERSET_BUG_REPORT_TEXT=Report a bug                              # (Report a bug)
#SUPERSET_DOCUMENTATION_URL=                                        # Documentation URL
#SUPERSET_DOCUMENTATION_TEXT=Documentation                          # (Documentation)
#SUPERSET_TROUBLESHOOTING_LINK=                                     # Troubleshooting page
#SUPERSET_PERMISSION_INSTRUCTIONS_LINK=                             # Permission help page

# ---------------------------------------------------------------------------
# 4. SMTP / Email (required for alerts & reports)
# ---------------------------------------------------------------------------

#SUPERSET_SMTP_HOST=localhost                                       # (localhost)
#SUPERSET_SMTP_PORT=25                                              # (25)
#SUPERSET_SMTP_USER=superset                                        # (superset)
#SUPERSET_SMTP_PASSWORD=superset                                    # (superset)
#SUPERSET_SMTP_MAIL_FROM=superset@superset.com                     # (superset@superset.com)
#SUPERSET_SMTP_STARTTLS=true                                        # (true)
#SUPERSET_SMTP_SSL=false                                            # (false)

# ---------------------------------------------------------------------------
# 5. Security / Proxy
# ---------------------------------------------------------------------------

# Set to true when running behind a reverse proxy (nginx, ALB, etc.)
#SUPERSET_ENABLE_PROXY_FIX=false                                    # (false)

# Cookie settings — enable SECURE for HTTPS deployments
#SUPERSET_SESSION_COOKIE_SECURE=false                               # (false)
#SUPERSET_SESSION_COOKIE_SAMESITE=Lax                               # (Lax)

# Talisman adds security headers (CSP, HSTS, etc.)
#SUPERSET_TALISMAN_ENABLED=true                                     # (true)

# Force HTTPS via Talisman (auto-enabled when ENABLE_PROXY_FIX=true)
#SUPERSET_FORCE_HTTPS=false                                         # (false)

# CORS origins for embedded dashboards (comma-separated)
#SUPERSET_CORS_EMBED_ORIGINS=                                       # e.g. https://app.example.com

# ---------------------------------------------------------------------------
# 5a. Rate Limiting
# ---------------------------------------------------------------------------

#SUPERSET_RATELIMIT_ENABLED=false                                   # (false)
#SUPERSET_RATELIMIT_REDIS_DB=2                                      # (2)
#SUPERSET_RATELIMIT_STORAGE_URI=                                    # Auto-configured from Redis

# ---------------------------------------------------------------------------
# 6. Feature Flags
# ---------------------------------------------------------------------------

#SUPERSET_FEATURE_THUMBNAILS=true                                   # (true) Dashboard/chart thumbnails
#SUPERSET_FEATURE_PLAYWRIGHT=true                                   # (true) Playwright for screenshots
#SUPERSET_FEATURE_TAGGING=false                                     # (false) Tag system for organizing
#SUPERSET_FEATURE_EMBEDDED=false                                    # (false) Embedded dashboards (iframe)
#SUPERSET_FEATURE_EMBEDDABLE_CHARTS=true                            # (true) Embeddable chart links
#SUPERSET_FEATURE_SSH_TUNNELING=false                               # (false) SSH tunnels for DB connections
#SUPERSET_FEATURE_TEMPLATE_PROCESSING=false                         # (false) Jinja templates in SQL Lab
#SUPERSET_FEATURE_GLOBAL_ASYNC_QUERIES=false                        # (false) WebSocket-based async queries
#SUPERSET_FEATURE_THUMBNAILS_SQLA_LISTENERS=true                    # (true) Auto-invalidate thumbnails

# ---------------------------------------------------------------------------
# 7. Screenshot / Reports
# ---------------------------------------------------------------------------

# Internal URL the headless browser uses to capture screenshots
#SUPERSET_WEBDRIVER_BASEURL=                                        # e.g. http://superset:8088/

# Public URL shown in email report links
#SUPERSET_WEBDRIVER_BASEURL_USER_FRIENDLY=                          # e.g. https://insights.example.com

# Email report customization
#SUPERSET_EMAIL_REPORTS_SUBJECT_PREFIX=[Report]                     # ([Report])
#SUPERSET_EMAIL_REPORTS_CTA=Explore in Superset                     # (Explore in Superset)

# Dry run mode — suppress actual delivery (for testing)
#SUPERSET_ALERT_DRY_RUN=false                                       # (false)

# Minimum interval between executions (minutes)
#SUPERSET_ALERT_MINIMUM_INTERVAL_MINUTES=                           # Alerts
#SUPERSET_REPORT_MINIMUM_INTERVAL_MINUTES=                          # Reports

# Fixed executor fallback chain
#SUPERSET_ALERT_REPORTS_USE_FIXED_EXECUTOR=false                    # (false)
#SUPERSET_ALERT_REPORTS_FIXED_EXECUTOR_USER=admin                   # (admin)

# Playwright settings
#SUPERSET_SCREENSHOT_PLAYWRIGHT_TIMEOUT_MS=                         # Timeout in ms
#SUPERSET_SCREENSHOT_PLAYWRIGHT_WAIT_EVENT=                         # load|domcontentloaded|networkidle

# Extra Chromium flags (space-separated)
#SUPERSET_WEBDRIVER_EXTRA_ARGS=                                     # e.g. --disable-gpu --no-sandbox

# ---------------------------------------------------------------------------
# 8. Guest / Embedded Dashboards
# ---------------------------------------------------------------------------

# JWT secret for guest tokens — required if using embedded dashboards
#SUPERSET_GUEST_TOKEN_JWT_SECRET=                                   # Generate a random secret
#SUPERSET_GUEST_TOKEN_JWT_EXP=300                                   # (300) Expiry in seconds
#SUPERSET_GUEST_ROLE_NAME=Public                                    # (Public) Guest role name

# ---------------------------------------------------------------------------
# 8a. Global Async Queries
# ---------------------------------------------------------------------------

#SUPERSET_GLOBAL_ASYNC_QUERIES_JWT_SECRET=                          # Required when enabled

# ---------------------------------------------------------------------------
# 9. SQL Lab
# ---------------------------------------------------------------------------

#SUPERSET_SQLLAB_DEFAULT_LIMIT=1000                                 # (1000) Default row limit
#SUPERSET_SQL_MAX_ROW=100000                                        # (100000) Maximum rows returned
#SUPERSET_SQLLAB_TIMEOUT=30                                         # (30) Query timeout in seconds

# ---------------------------------------------------------------------------
# 9a. Results Backend
# ---------------------------------------------------------------------------
# Use Redis instead of filesystem for query results (required for multi-worker)

#SUPERSET_RESULTS_BACKEND_USE_REDIS=false                           # (false)
#REDIS_RESULTS_DB=1                                                 # (1) Redis DB number

# ---------------------------------------------------------------------------
# 10. Localization
# ---------------------------------------------------------------------------

#SUPERSET_BABEL_DEFAULT_LOCALE=en                                   # (en)

# ---------------------------------------------------------------------------
# 11. Integrations
# ---------------------------------------------------------------------------

#SUPERSET_MAPBOX_API_KEY=                                           # Mapbox (geospatial charts)
#SUPERSET_SLACK_API_TOKEN=                                          # Slack (alert/report delivery)

# ---------------------------------------------------------------------------
# 12. Environment Tag
# ---------------------------------------------------------------------------
# Shows a colored badge in the navbar. Values: development, staging, production

#SUPERSET_ENVIRONMENT_TAG=                                          # Leave unset for no badge
